    "homepage": "https://github.com/kornellrus/laraveltranslate",
    "keywords": ["Laravel", "LaravelTranslate", "Translate", "google"],
    "require": {
        "illuminate/support": "~7|~8",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "KornellRus\\LaravelTranslate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KornellRus\\LaravelTranslate\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KornellRus\\LaravelTranslate\\LaravelTranslateServiceProvider"
            ],
            "aliases": {
                "LaravelTranslate": "KornellRus\\LaravelTranslate\\Facades\\LaravelTranslate"
            }
        }
    }
}
